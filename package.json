{
  "name": "pet_be",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./tests/unit/jest.json",
    "test:all": "jest --config ./tests/unit/jest.json",
    "test:changed": "bash scripts/test-changed.sh",
    "test:unit": "jest --config ./tests/unit/jest.json",
    "test:services": "jest --config ./tests/unit/jest.json tests/unit/services/",
    "test:controllers": "jest --config ./tests/unit/jest.json tests/unit/controllers/",
    "test:integration": "jest --config ./tests/integration/jest.json",
    "test:integration:with-db": "docker-compose -f docker-compose.e2e.yml up -d && npx wait-on tcp:5433 && npm run test:integration && docker-compose -f docker-compose.e2e.yml down",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:cov:all": "npm run test:cov && npm run test:integration && npm run test:e2e",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./tests/e2e/jest.json",
    "test:e2e:with-db": "docker-compose -f docker-compose.e2e.yml up -d && npx wait-on tcp:5433 && npm run test:e2e && docker-compose -f docker-compose.e2e.yml down",
    "seed": "ts-node -r tsconfig-paths/register src/database/run-seed.ts",
    "seed:refresh": "ts-node -r tsconfig-paths/register src/database/run-seed.ts --refresh",
    "seed:demo": "ts-node -r tsconfig-paths/register src/database/run-seed-demo.ts",
    "seed:demo:refresh": "ts-node -r tsconfig-paths/register src/database/run-seed-demo.ts --refresh"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/typeorm": "^11.0.0",
    "@types/bull": "^3.15.9",
    "@types/nodemailer": "^7.0.5",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "handlebars": "^4.7.8",
    "ioredis": "^5.9.1",
    "nestjs-i18n": "^10.6.0",
    "nestjs-vnpay": "^1.0.4",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "wait-on": "^7.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  }
}
